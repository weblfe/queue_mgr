        "/consumers": {
            "get": {
                "description": "query queue consumer lists",
                "consumes": [
                    "application/x-www-form-urlencoded"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "QueueMgrServ"
                ],
                "summary": "罗列消费队列信息",
                "parameters": [
                    {
                        "type": "string",
                        "description": "access jwt token",
                        "name": "Authorization",
                        "in": "header",
                        "required": true
                    },
                    {
                        "type": "integer",
                        "default": 1,
                        "description": "page/页码",
                        "name": "page",
                        "in": "formData"
                    },
                    {
                        "type": "integer",
                        "default": 10,
                        "description": "count/分页量",
                        "name": "count",
                        "in": "query"
                    },
                    {
                        "enum": [
                            0,
                            1,
                            2,
                            3
                        ],
                        "type": "integer",
                        "description": "state/消费进程状态",
                        "name": "state",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "default": "\"created_at:desc\"",
                        "description": "sort/排序参数",
                        "name": "sort",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "name/限定队列名(模糊匹配eg: test*)",
                        "name": "queue",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
